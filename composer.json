{
    "name": "me12free/laravel-mpesa-stkpush-production",
    "description": "A premium Laravel package for secure, production-ready M-Pesa STK Push integration.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {"name": "John Ekiru", "email": "johnewoi72@gmail.com"}
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "MpesaPremium\\": "src/"
        }
    },
    "extra": {
        "laravel": {"providers": ["MpesaPremium\\MpesaPremiumServiceProvider"]}
    },
    "require-dev": {
        "orchestra/testbench": "^10.0 || ^11.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/me12free/laravel-mpesa-stkpush-production/issues",
        "source": "https://github.com/me12free/laravel-mpesa-stkpush-production"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/me12free/laravel-mpesa-stkpush-production.git"
        }
    ]
}
